{
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
    }
  },
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": true,
  "deno.config": "./deno.json",
  "deno.suggest.imports.hosts": {
    "https://deno.land": false
  }
}
